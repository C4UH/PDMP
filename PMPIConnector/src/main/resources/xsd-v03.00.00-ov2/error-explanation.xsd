  xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
  xmlns:pmpi="http://pmpi.nabp.org/">

  <xsd:element name="ErrorExplanation">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="Errors" minOccurs="1" maxOccurs="1">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="Error" type="xsd:string" />
